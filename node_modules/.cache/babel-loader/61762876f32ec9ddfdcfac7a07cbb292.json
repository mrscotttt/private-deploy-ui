{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport draggable from \"vuedraggable\";\nexport default {\n  name: 'dragTicket',\n  components: {\n    draggable\n  },\n  props: ['ticket'],\n  data: () => ({\n    list1: [{\n      name: \"John\",\n      id: 1\n    }, {\n      name: \"Joao\",\n      id: 2\n    }, {\n      name: \"Jean\",\n      id: 3\n    }, {\n      name: \"Gerard\",\n      id: 4\n    }],\n    list2: [{\n      name: \"Juan\",\n      id: 5\n    }, {\n      name: \"Edgard\",\n      id: 6\n    }, {\n      name: \"Johnson\",\n      id: 7\n    }],\n    report: []\n  }),\n  computed: {\n    ticketGroup() {\n      let t = this.ticket;\n      let group = {\n        pending: g1,\n        accepted: g2,\n        resolved: g3,\n        rejected: g4\n      };\n      for (var i = 0; i < t.lenght; i++) {\n        if (item.status == \"pending\") {\n          g1.push(item);\n        } else if (item.status == \"accepted\") {\n          g2.push(item);\n        } else if (item.status == \"resolved\") {\n          g3.push(item);\n        } else if (item.status == \"rejected\") {\n          g4.push(item);\n        }\n      }\n      return group;\n    }\n  },\n  methods: {\n    add: function () {\n      this.list.push({\n        name: \"Juan\"\n      });\n    },\n    replace: function () {\n      this.list = [{\n        name: \"Edgard\"\n      }];\n    },\n    clone: function (el) {\n      return {\n        name: el.name + \" cloned\"\n      };\n    },\n    log: function (evt) {\n      this.report.push(evt);\n      console.log(evt);\n    }\n  }\n};","map":{"version":3,"mappings":";AA0CA;AACA;EACAA;EACAC;IAAAC;EAAA;EACAC;EACAC;IACAC,QACA;MAAAL;MAAAM;IAAA,GACA;MAAAN;MAAAM;IAAA,GACA;MAAAN;MAAAM;IAAA,GACA;MAAAN;MAAAM;IAAA,EACA;IACAC,QACA;MAAAP;MAAAM;IAAA,GACA;MAAAN;MAAAM;IAAA,GACA;MAAAN;MAAAM;IAAA,EACA;IACAE;EACA;EACAC;IACAC;MACA;MACA;QAAAC;QAAAC;QAAAC;QAAAC;MAAA;MACA;QACA;UAAAC;QAAA,OACA;UAAAC;QAAA,OACA;UAAAC;QAAA,OACA;UAAAC;QAAA;MACA;MAEA;IACA;EACA;EAEAC;IACAC;MACA;QAAApB;MAAA;IACA;IACAqB;MACA;QAAArB;MAAA;IACA;IACAsB;MACA;QACAtB;MACA;IACA;IACAuB;MACA;MACAC;IACA;EACA;AACA","names":["name","components","draggable","props","data","list1","id","list2","report","computed","ticketGroup","pending","accepted","resolved","rejected","g1","g2","g3","g4","methods","add","replace","clone","log","console"],"sourceRoot":"src/components","sources":["dragTicket.vue"],"sourcesContent":["<template>\r\n    <div class=\"p-16\">\r\n        <div class=\"rounded-lg dragArea w-full justify-center ma-16 p-8 text-black drop-shadow-2xl backdrop-blur-md bg-white/60\">\r\n            <!-- KU Help-Desk Centeres {{columns.pending}} -->\r\n            {{report}}\r\n            {{ticketGroup}}\r\n            <div class=\"row\">\r\n                <div class=\"col-3\">\r\n                  <h3>Draggable 1</h3>\r\n                  <draggable class=\"list-group bg-rose-200\" :list=\"list1\" group=\"people\" @change=\"log\">\r\n                    <div\r\n                      class=\"list-group-item\"\r\n                      v-for=\"(element, index) in list1\"\r\n                      :key=\"element.name\"\r\n                    >\r\n                      {{ element.name }} {{ index }}\r\n                    </div>\r\n                  </draggable>\r\n                </div>\r\n            \r\n                <div class=\"col-3\">\r\n                  <h3>Draggable 2</h3>\r\n                  <draggable class=\"list-group bg-indigo-200\" :list=\"list2\" group=\"people\" @change=\"log\">\r\n                    <div\r\n                      class=\"list-group-item\"\r\n                      v-for=\"(element, index) in list2\"\r\n                      :key=\"element.name\"\r\n                    >\r\n                      {{ element.name }} {{ index }}\r\n                    </div>\r\n                  </draggable>\r\n                </div>\r\n            \r\n                <!-- <rawDisplayer class=\"col-3\" :value=\"list1\" title=\"List 1\" />\r\n                <rawDisplayer class=\"col-3\" :value=\"list2\" title=\"List 2\" /> -->\r\n            </div>\r\n            \r\n        </div>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  import draggable from \"vuedraggable\";\r\n  export default {\r\n    name: 'dragTicket',\r\n    components: {draggable},\r\n    props: ['ticket'],\r\n    data: () => ({\r\n      list1: [\r\n        { name: \"John\", id: 1 },\r\n        { name: \"Joao\", id: 2 },\r\n        { name: \"Jean\", id: 3 },\r\n        { name: \"Gerard\", id: 4 }\r\n      ],\r\n      list2: [\r\n        { name: \"Juan\", id: 5 },\r\n        { name: \"Edgard\", id: 6 },\r\n        { name: \"Johnson\", id: 7 }\r\n      ],\r\n      report: []\r\n    }),\r\n    computed: {\r\n        ticketGroup() {\r\n            let t = this.ticket\r\n            let group = {pending: g1,accepted: g2,resolved: g3,rejected: g4}\r\n            for(var i=0;i<t.lenght;i++){\r\n                if(item.status==\"pending\"){(g1).push(item)}\r\n                else if(item.status==\"accepted\"){(g2).push(item)}\r\n                else if(item.status==\"resolved\"){(g3).push(item)}\r\n                else if(item.status==\"rejected\"){(g4).push(item)}\r\n            }\r\n            \r\n            return group\r\n        },\r\n    },\r\n\r\n    methods: {\r\n        add: function() {\r\n        this.list.push({ name: \"Juan\" });\r\n        },\r\n        replace: function() {\r\n        this.list = [{ name: \"Edgard\" }];\r\n        },\r\n        clone: function(el) {\r\n        return {\r\n            name: el.name + \" cloned\"\r\n        };\r\n        },\r\n        log: function(evt) {\r\n            this.report.push(evt)\r\n            console.log(evt)\r\n    }\r\n  }\r\n  }\r\n  </script>\r\n  \r\n  <style>\r\n  .dragArea{\r\n    height: 720px !important;\r\n  }\r\n  .column-width {\r\n    min-width: 320px;\r\n    width: 320px;\r\n  }\r\n  .ghost-card {\r\n    opacity: 0.5;\r\n    background: #F7FAFC;\r\n    border: 1px solid #4299e1;\r\n  }\r\n  </style>\r\n  "]},"metadata":{},"sourceType":"module"}