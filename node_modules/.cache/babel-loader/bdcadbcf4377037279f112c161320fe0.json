{"ast":null,"code":"import dragTicket from './components/dragTicket.vue';\nimport axios from 'axios';\nexport default {\n  name: 'App',\n  components: {\n    dragTicket\n  },\n  data: () => ({\n    info: null,\n    loading: true,\n    errored: false,\n    dialog: false,\n    animaltype: [\"Dog\", \"Cat\", \"Fish\", \"Bird\", \"Mouse\"],\n    petForm: {\n      petName: null,\n      petType: null\n    },\n    lastpetid: null,\n    lasttype: null\n  }),\n  methods: {\n    hookpet() {\n      axios.get('/pet').then(response => this.info = response).catch(error => {\n        console.log(error);\n        this.errored = true;\n      }).finally(() => this.loading = false);\n    },\n    delpet(id) {\n      const text = \"Confirm to delete this pet information?\";\n      const url = '/delpet/' + id.toString();\n      if (confirm(text) == true) {\n        axios.delete(url).then(response => console.log(response)).then(location.reload()).catch(error => {\n          console.log(error);\n          this.errored = true;\n        });\n      }\n    },\n    submitForm(id) {\n      const newPet = {\n        petName: this.petForm.petName,\n        petType: this.petForm.petType\n      };\n\n      //put\n      const article = newPet;\n      const url = \"/updatepet/\" + id.toString();\n      axios.put(url, article).then(response => this.articleId = response.data.id);\n      alert(\"Confirm to update pet information.\");\n      location.reload();\n    }\n  },\n  created: async function () {\n    await this.hookpet();\n  }\n};","map":{"version":3,"mappings":"AA2CA;AACA;AAEA;EACAA;EACAC;IAAAC;EAAA;EACAC;IACAC;IACAC;IACAC;IACAC;IACAC;IAEAC;MACAC;MACAC;IACA;IAEAC;IACAC;EAEA;EAEAC;IACAC;MACAC,MACAC,YACAC,uCACAC;QACAC;QACA;MACA,GACAC;IACA;IAEAC;MACA;MACA;MACA;QACAN,MACAO,YACAL,wCACAA,wBACAC;UACAC;UACA;QACA;MACA;IACA;IAEAI;MACA;QACAd;QACAC;MACA;;MAEA;MACA;MACA;MACAK,wBACAE;MAEAO;MACAC;IACA;EACA;EACAC;IACA;EACA;AACA","names":["name","components","dragTicket","data","info","loading","errored","dialog","animaltype","petForm","petName","petType","lastpetid","lasttype","methods","hookpet","axios","get","then","catch","console","finally","delpet","delete","submitForm","alert","location","created"],"sourceRoot":"src","sources":["App.vue"],"sourcesContent":["<template>\n  <div id=\"app\">\n\n    <div class=\"h-screen w-screen justify-center bg-gradient-to-r\n                  from-teal-500\n                  via-teal-300\n                  to-teal-100\n                  background-animate\">\n      <nav class=\"bg-white border-gray-200 px-2 sm:px-4 py-2.5 dark:bg-gray-900\">\n        <div class=\"container flex flex-wrap justify-between items-center mx-auto\">\n          <div class=\"hidden w-full md:block md:w-auto\" id=\"navbar-default\">\n            <ul class=\"flex flex-col p-4 mt-4 bg-gray-50 rounded-lg border border-gray-100 md:flex-row md:space-x-8 md:mt-0 md:text-sm md:font-medium md:border-0 md:bg-white dark:bg-gray-800 md:dark:bg-gray-900 dark:border-gray-700\">\n              <li>\n                <a href=\"#\" class=\"block py-2 pr-4 pl-3 text-white bg-blue-700 rounded md:bg-transparent md:text-blue-700 md:p-0 dark:text-white\" aria-current=\"page\">Home</a>\n              </li>\n              <li>\n                <a href=\"#\" class=\"block py-2 pr-4 pl-3 text-gray-700 rounded hover:bg-gray-100 md:hover:bg-transparent md:border-0 md:hover:text-blue-700 md:p-0 dark:text-gray-400 md:dark:hover:text-white dark:hover:bg-gray-700 dark:hover:text-white md:dark:hover:bg-transparent\">About</a>\n              </li>\n              <li>\n                <a href=\"#\" class=\"block py-2 pr-4 pl-3 text-gray-700 rounded hover:bg-gray-100 md:hover:bg-transparent md:border-0 md:hover:text-blue-700 md:p-0 dark:text-gray-400 md:dark:hover:text-white dark:hover:bg-gray-700 dark:hover:text-white md:dark:hover:bg-transparent\">Services</a>\n              </li>\n              <li>\n                <a href=\"#\" class=\"block py-2 pr-4 pl-3 text-gray-700 rounded hover:bg-gray-100 md:hover:bg-transparent md:border-0 md:hover:text-blue-700 md:p-0 dark:text-gray-400 md:dark:hover:text-white dark:hover:bg-gray-700 dark:hover:text-white md:dark:hover:bg-transparent\">Pricing</a>\n              </li>\n              <li>\n                <a href=\"#\" class=\"block py-2 pr-4 pl-3 text-gray-700 rounded hover:bg-gray-100 md:hover:bg-transparent md:border-0 md:hover:text-blue-700 md:p-0 dark:text-gray-400 md:dark:hover:text-white dark:hover:bg-gray-700 dark:hover:text-white md:dark:hover:bg-transparent\">Contact</a>\n              </li>\n            </ul>\n          </div>\n        </div>\n      </nav>\n      <!-- <div class=\"p-16\">\n        <div class=\"rounded-lg dragArea w-full justify-center ma-16 p-8 text-black drop-shadow-2xl backdrop-blur-md bg-white/60\">\n          KU Help-Desk Center\n        </div>\n      </div> -->\n      <dragTicket></dragTicket>\n    </div>\n  </div>\n</template>\n\n<script>\n\nimport dragTicket from './components/dragTicket.vue';\nimport axios from 'axios'\n\nexport default {\n  name: 'App',\n  components: {dragTicket},\n  data: () => ({\n      info: null,\n      loading: true,\n      errored: false,\n      dialog: false,\n      animaltype: [\"Dog\",\"Cat\",\"Fish\",\"Bird\",\"Mouse\"],\n\n      petForm:{\n        petName:null,\n        petType:null\n      },\n\n      lastpetid:null,\n      lasttype:null,\n\n    }),\n\n    methods:{\n        hookpet () {\n            axios\n            .get('/pet')\n            .then(response => (this.info = response))\n            .catch(error => {\n              console.log(error)\n              this.errored = true\n            })\n            .finally(() => this.loading = false)\n        },\n\n        delpet(id){\n            const text = \"Confirm to delete this pet information?\";\n            const url = '/delpet/' + id.toString()\n            if (confirm(text) == true) {\n                axios\n                .delete(url)\n                .then(response => (console.log(response)))\n                .then(location.reload())\n                .catch(error => {\n                  console.log(error)\n                  this.errored = true\n                })  \n          }\n        },\n\n        submitForm(id){\n            const newPet={\n                petName:this.petForm.petName,\n                petType:this.petForm.petType,\n            }\n\n            //put\n            const article = newPet;\n            const url = \"/updatepet/\"+id.toString()\n            axios.put(url, article)\n              .then(response => this.articleId = response.data.id);\n            \n            alert(\"Confirm to update pet information.\");\n            location.reload();\n        },\n    },\n    created: async function(){\n        await this.hookpet()\n      }\n}\n</script>\n\n<style>\nbody {\n  font-family: \"Inter\", sans-serif;\n}\n\n.background-animate {\n  background-size: 400%;\n\n  -webkit-animation: AnimationName 20s ease infinite;\n  -moz-animation: AnimationName 20s ease infinite;\n  animation: AnimationName 20s ease infinite;\n}\n\n@keyframes AnimationName {\n  0%,\n  100% {\n    background-position: 0% 50%;\n  }\n  50% {\n    background-position: 100% 50%;\n  }\n}\n\n.dragArea{\n  height: 720px !important;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}